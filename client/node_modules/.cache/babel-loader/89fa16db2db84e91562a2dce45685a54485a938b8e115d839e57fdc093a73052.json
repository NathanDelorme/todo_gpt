{"ast":null,"code":"import TaskDataService from '@/services/TaskDataService';\nexport default {\n  name: 'TaskCreatorComponent',\n  props: ['tasks', 'todolistID'],\n  data() {\n    return {\n      newTask: {\n        title: '',\n        priority: 0\n      }\n    };\n  },\n  methods: {\n    createTask() {\n      TaskDataService.create(this.newTask).then(response => {\n        TaskDataService.getAllOfTodolist(this.todolistID).then(response => {\n          this.$store.dispatch('tasks', {\n            tasks: response.data.tasks,\n            idTodolist: this.todolistID\n          });\n        });\n      });\n    }\n  }\n};","map":{"version":3,"names":["TaskDataService","name","props","data","newTask","title","priority","methods","createTask","create","then","response","getAllOfTodolist","todolistID","$store","dispatch","tasks","idTodolist"],"sources":["C:\\Users\\natha\\Desktop\\todo_gpt\\client\\src\\components\\TaskCreatorComponent.vue"],"sourcesContent":["<template>\r\n  <button type=\"button\" class=\"btn btn-outline-primary\" data-bs-toggle=\"modal\" :data-bs-target=\"'#taskCreator' + this.todolistID\" >Create Task</button>\r\n\r\n  <div class=\"modal fade\" :id=\"'taskCreator' + this.todolistID\" data-bs-backdrop=\"static\" data-bs-keyboard=\"false\" tabindex=\"-1\">\r\n    <div class=\"modal-dialog\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n          <h5 class=\"modal-title\">Task Creator</h5>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <div class=\"form-group\">\r\n            <label for=\"titleTaskInput\">Title :</label>\r\n            <input type=\"text\" class=\"form-control text-center\" id=\"titleTaskInput\" v-model=\"this.newTask.title\">\r\n            <select class=\"form-select form-select-sm\" v-model.number=\"this.newTask.priority\">\r\n              <option value=\"2\">Urgent</option>\r\n              <option value=\"1\">Important</option>\r\n              <option value=\"0\">Not important</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button type=\"button\" class=\"btn btn-primary\" :disabled=\"!newTask.title\" @click=\"createTask\" data-bs-toggle=\"modal\" :data-bs-target=\"'#taskCreator' + this.todolistID\">Create to do list</button>\r\n          <button type=\"button\" class=\"btn btn-secondary\" data-bs-toggle=\"modal\" :data-bs-target=\"'#taskCreator' + this.todolistID\">Close</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TaskDataService from '@/services/TaskDataService'\r\n\r\nexport default {\r\n  name: 'TaskCreatorComponent',\r\n  props: ['tasks', 'todolistID'],\r\n  data () {\r\n    return {\r\n      newTask: {\r\n        title: '',\r\n        priority: 0\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    createTask () {\r\n      TaskDataService.create(this.newTask).then(response => {\r\n        TaskDataService.getAllOfTodolist(this.todolistID).then(response => {\r\n          this.$store.dispatch('tasks', {\r\n            tasks: response.data.tasks,\r\n            idTodolist: this.todolistID\r\n          })\r\n        })\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AA8BA,OAAOA,eAAc,MAAO,4BAA2B;AAEvD,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,KAAK,EAAE,CAAC,OAAO,EAAE,YAAY,CAAC;EAC9BC,IAAGA,CAAA,EAAK;IACN,OAAO;MACLC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,UAASA,CAAA,EAAK;MACZR,eAAe,CAACS,MAAM,CAAC,IAAI,CAACL,OAAO,CAAC,CAACM,IAAI,CAACC,QAAO,IAAK;QACpDX,eAAe,CAACY,gBAAgB,CAAC,IAAI,CAACC,UAAU,CAAC,CAACH,IAAI,CAACC,QAAO,IAAK;UACjE,IAAI,CAACG,MAAM,CAACC,QAAQ,CAAC,OAAO,EAAE;YAC5BC,KAAK,EAAEL,QAAQ,CAACR,IAAI,CAACa,KAAK;YAC1BC,UAAU,EAAE,IAAI,CAACJ;UACnB,CAAC;QACH,CAAC;MACH,CAAC;IACH;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}