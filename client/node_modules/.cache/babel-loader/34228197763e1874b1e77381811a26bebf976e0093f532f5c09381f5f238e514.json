{"ast":null,"code":"import TodoListComponent from '@/components/TodoListComponent.vue';\nimport TodolistDataService from '@/services/TodolistDataService';\nimport ToDoListCreatorComponent from '@/components/ToDoListCreatorComponent';\nimport TaskDataService from '@/services/TaskDataService';\nimport GPTTasksCreatorComponent from '@/components/GPTTaskCreatorComponent';\nexport default {\n  components: {\n    GPTTasksCreatorComponent,\n    ToDoListCreatorComponent,\n    TodoListComponent\n  },\n  name: 'HomePage',\n  props: ['user', 'todolists', 'tasks'],\n  methods: {\n    deleteTodolist(id) {\n      TodolistDataService.delete(id).then(response => {\n        TodolistDataService.getAllOfUser(this.user.idUser).then(response => {\n          this.$store.dispatch('todolists', response.data.todolists);\n        });\n      });\n    },\n    updateTodolist(todolist) {\n      todolist.lastModifiedDate = new Date();\n      TodolistDataService.update(todolist.idTodolist, todolist).then(response => {});\n    },\n    createTodolist(todolist) {\n      todolist.idUser = this.user.idUser;\n      TodolistDataService.create(todolist).then(response => {\n        TodolistDataService.getAllOfUser(this.user.idUser).then(response => {\n          this.$store.dispatch('todolists', response.data.todolists);\n        });\n      });\n    },\n    createTask(task) {\n      TaskDataService.create(task).then(response => {\n        this.$store.dispatch('createTask', response.data);\n      });\n    },\n    deleteTask(task) {\n      TaskDataService.delete(task.idTask).then(response => {\n        this.$store.dispatch('removeTask', task);\n      });\n    },\n    updateTask(task) {\n      TaskDataService.update(task.idTask, task).then(response => {});\n    },\n    requestChatGPT(data) {\n      TaskDataService.requestGPT(data).then(response => {\n        for (const task of response.data.tasks) {\n          this.$store.dispatch('createTask', task);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["TodoListComponent","TodolistDataService","ToDoListCreatorComponent","TaskDataService","GPTTasksCreatorComponent","components","name","props","methods","deleteTodolist","id","delete","then","response","getAllOfUser","user","idUser","$store","dispatch","data","todolists","updateTodolist","todolist","lastModifiedDate","Date","update","idTodolist","createTodolist","create","createTask","task","deleteTask","idTask","updateTask","requestChatGPT","requestGPT","tasks"],"sources":["C:\\Users\\natha\\Desktop\\todo_gpt\\client\\src\\views\\HomePage.vue"],"sourcesContent":["<template>\r\n  <main v-if=\"user\" class=\"container mt-5\">\r\n    <section>\r\n      <h1 class=\"text-center mb-4\">Todo GPT - AI based Todo List application !</h1>\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-md-6\">\r\n          <p class=\"text-center\">Logged as {{ user.email }}</p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section>\r\n      <div class=\"d-grid gap-3\">\r\n        <GPTTasksCreatorComponent :todolists=\"todolists\" v-on:request-chat-gpt=\"requestChatGPT\"/>\r\n        <ToDoListCreatorComponent v-on:create-to-do-list=\"createTodolist\"/>\r\n        <div>\r\n          <TodoListComponent v-for=\"(todolist) in todolists\"\r\n                             :key=\"todolist.idTodolist\"\r\n                             :id=\"todolist.idTodolist\"\r\n                             :todolists=\"todolists\"\r\n                             :isEditing=\"false\"\r\n                             :showTasks=\"false\"\r\n                             :user=\"user\"\r\n                             :tasks=\"tasks\"\r\n                             v-on:remove-to-do-list=\"deleteTodolist\"\r\n                             v-on:update-to-do-list=\"updateTodolist\"\r\n                             v-on:create-task=\"createTask\"\r\n                             v-on:delete-task=\"deleteTask\"\r\n                             v-on:update-task=\"updateTask\"/>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </main>\r\n  <div v-else>{{ this.$router.push({ name: 'login' }) }}</div>\r\n</template>\r\n\r\n<script>\r\nimport TodoListComponent from '@/components/TodoListComponent.vue'\r\nimport TodolistDataService from '@/services/TodolistDataService'\r\nimport ToDoListCreatorComponent from '@/components/ToDoListCreatorComponent'\r\nimport TaskDataService from '@/services/TaskDataService'\r\nimport GPTTasksCreatorComponent from '@/components/GPTTaskCreatorComponent'\r\n\r\nexport default {\r\n  components: { GPTTasksCreatorComponent, ToDoListCreatorComponent, TodoListComponent },\r\n  name: 'HomePage',\r\n  props: ['user', 'todolists', 'tasks'],\r\n  methods: {\r\n    deleteTodolist (id) {\r\n      TodolistDataService.delete(id).then(response => {\r\n        TodolistDataService.getAllOfUser(this.user.idUser).then(response => {\r\n          this.$store.dispatch('todolists', response.data.todolists)\r\n        })\r\n      })\r\n    },\r\n    updateTodolist (todolist) {\r\n      todolist.lastModifiedDate = new Date()\r\n      TodolistDataService.update(todolist.idTodolist, todolist).then(response => {})\r\n    },\r\n    createTodolist (todolist) {\r\n      todolist.idUser = this.user.idUser\r\n      TodolistDataService.create(todolist).then(response => {\r\n        TodolistDataService.getAllOfUser(this.user.idUser).then(response => {\r\n          this.$store.dispatch('todolists', response.data.todolists)\r\n        })\r\n      })\r\n    },\r\n    createTask (task) {\r\n      TaskDataService.create(task).then(response => {\r\n        this.$store.dispatch('createTask', response.data)\r\n      })\r\n    },\r\n    deleteTask (task) {\r\n      TaskDataService.delete(task.idTask).then(response => {\r\n        this.$store.dispatch('removeTask', task)\r\n      })\r\n    },\r\n    updateTask (task) {\r\n      TaskDataService.update(task.idTask, task).then(response => { })\r\n    },\r\n    requestChatGPT (data) {\r\n      TaskDataService.requestGPT(data).then(response => {\r\n        for (const task of response.data.tasks) {\r\n          this.$store.dispatch('createTask', task)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAoCA,OAAOA,iBAAgB,MAAO,oCAAmC;AACjE,OAAOC,mBAAkB,MAAO,gCAA+B;AAC/D,OAAOC,wBAAuB,MAAO,uCAAsC;AAC3E,OAAOC,eAAc,MAAO,4BAA2B;AACvD,OAAOC,wBAAuB,MAAO,sCAAqC;AAE1E,eAAe;EACbC,UAAU,EAAE;IAAED,wBAAwB;IAAEF,wBAAwB;IAAEF;EAAkB,CAAC;EACrFM,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,MAAM,EAAE,WAAW,EAAE,OAAO,CAAC;EACrCC,OAAO,EAAE;IACPC,cAAaA,CAAGC,EAAE,EAAE;MAClBT,mBAAmB,CAACU,MAAM,CAACD,EAAE,CAAC,CAACE,IAAI,CAACC,QAAO,IAAK;QAC9CZ,mBAAmB,CAACa,YAAY,CAAC,IAAI,CAACC,IAAI,CAACC,MAAM,CAAC,CAACJ,IAAI,CAACC,QAAO,IAAK;UAClE,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,WAAW,EAAEL,QAAQ,CAACM,IAAI,CAACC,SAAS;QAC3D,CAAC;MACH,CAAC;IACH,CAAC;IACDC,cAAaA,CAAGC,QAAQ,EAAE;MACxBA,QAAQ,CAACC,gBAAe,GAAI,IAAIC,IAAI,EAAC;MACrCvB,mBAAmB,CAACwB,MAAM,CAACH,QAAQ,CAACI,UAAU,EAAEJ,QAAQ,CAAC,CAACV,IAAI,CAACC,QAAO,IAAK,CAAC,CAAC;IAC/E,CAAC;IACDc,cAAaA,CAAGL,QAAQ,EAAE;MACxBA,QAAQ,CAACN,MAAK,GAAI,IAAI,CAACD,IAAI,CAACC,MAAK;MACjCf,mBAAmB,CAAC2B,MAAM,CAACN,QAAQ,CAAC,CAACV,IAAI,CAACC,QAAO,IAAK;QACpDZ,mBAAmB,CAACa,YAAY,CAAC,IAAI,CAACC,IAAI,CAACC,MAAM,CAAC,CAACJ,IAAI,CAACC,QAAO,IAAK;UAClE,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,WAAW,EAAEL,QAAQ,CAACM,IAAI,CAACC,SAAS;QAC3D,CAAC;MACH,CAAC;IACH,CAAC;IACDS,UAASA,CAAGC,IAAI,EAAE;MAChB3B,eAAe,CAACyB,MAAM,CAACE,IAAI,CAAC,CAAClB,IAAI,CAACC,QAAO,IAAK;QAC5C,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAEL,QAAQ,CAACM,IAAI;MAClD,CAAC;IACH,CAAC;IACDY,UAASA,CAAGD,IAAI,EAAE;MAChB3B,eAAe,CAACQ,MAAM,CAACmB,IAAI,CAACE,MAAM,CAAC,CAACpB,IAAI,CAACC,QAAO,IAAK;QACnD,IAAI,CAACI,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAEY,IAAI;MACzC,CAAC;IACH,CAAC;IACDG,UAASA,CAAGH,IAAI,EAAE;MAChB3B,eAAe,CAACsB,MAAM,CAACK,IAAI,CAACE,MAAM,EAAEF,IAAI,CAAC,CAAClB,IAAI,CAACC,QAAO,IAAK,CAAE,CAAC;IAChE,CAAC;IACDqB,cAAaA,CAAGf,IAAI,EAAE;MACpBhB,eAAe,CAACgC,UAAU,CAAChB,IAAI,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QAChD,KAAK,MAAMiB,IAAG,IAAKj